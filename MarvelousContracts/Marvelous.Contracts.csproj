    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Students</Authors>
    <Company>DevEducation</Company>
    <Description>Contracts for Marvelous microservice solution</Description>
    <PackageIcon>photo_2022-01-12_17-26-25.jpg</PackageIcon>
