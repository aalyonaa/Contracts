  <ItemGroup>
    <PackageReference Include="RestSharp" Version="107.3.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="photo_2022-01-12_17-26-25.jpg" Link="photo_2022-01-12_17-26-25.jpg">
